#: authentication/views/account_view.py:52
msgid "Email Activation"
msgstr "Email de Ativação"

#: authentication/templates/authentication/account/activation.html:6
#, python-format
msgid ""
"\n"
"\t\t<p>Dear %(user)s</p>\n"
"\t\t<p>Your account has successfully created. Please click below link to "
"activate your account.</p>\n"
"\t\t<p><a href=\"%(activate_url)s\"> Activate your account </a></p>\n"
"    "
msgstr ""
"\n"
"<p>Querido %(user)s</p>\n"
"<p>Sua conta foi criada com sucesso. Por favor, clique nesse link para "
"ativar sua conta.</p>\n"
"<p><a href=\"%(activate_url)s\"> Ativar sua conta </a></p>"

#: authentication/templates/authentication/account/check_delete.html:15
#: authentication/templates/authentication/account/check_delete.html:19
msgid "Confirm delete"
msgstr "Confirmação de exclusão"

#: authentication/templates/authentication/account/check_delete.html:27
msgid "Are you sure you want to delete"
msgstr "Você deseja delete isso?"

#: authentication/templates/authentication/account/check_delete.html:29
msgid "No"
msgstr "Não"

#: authentication/templates/authentication/account/check_delete.html:32
msgid "Yes"
msgstr "Sim"

